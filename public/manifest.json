{
    "manifest_version": 3,
    "name": "Solus",
    "version": "1.0.0",
    "description": "A powerful AI assistant",
    "action": {
      "default_popup": "index.html",
      "default_icon": {
      "16": "icon-16.png",
      "32": "icon-32.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    }
    },
    "permissions": ["identity","cookies","offscreen","geolocation","scripting","tabs"],
    "host_permissions":["http://localhost:8000/auth/login"],
    "oauth2": {
      "client_id": "267309487358-27j43bjgpnb03a4lqevcoqci4g2nu8jm.apps.googleusercontent.com",
      "scopes": ["profile", "email"]
    },
    "background":{
      "service_worker":"src/script/background.js"
    },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/script/content.js"]
    }
  ]
}